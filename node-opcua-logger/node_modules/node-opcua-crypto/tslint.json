{
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      false,
      "never-prefix"
    ],
    "interface-over-type-literal": true,
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore"
    ],
    "trailing-comma": [
      false
    ],
    "object-literal-sort-keys": false,
    "comment-format": [
      false
    ],
    "no-var-requires": false,
    "max-line-length": [
      false,
      120
    ],
    "one-variable-per-declaration": [
      false
    ]
  },
  "rulesDirectory": []
}
