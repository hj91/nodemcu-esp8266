{
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      false,
      "never-prefix"
    ],
    "interface-over-type-literal": true,
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore"
    ],
    "trailing-comma": [
      true
    ],
    "member-ordering": [
      true,
      {
        "order": [
          "public-static-field",
          "public-instance-field",
          "public-constructor",
          "private-static-field",
          "private-instance-field",
          "private-constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ]
  },
  "rulesDirectory": [
  ]
}
